@model IEnumerable<Khanhlyluyentap1.Models.SinhVien>

@{
    ViewData["Title"] = "Index";
}

<h1>MM</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenSinhVien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GioiTinh)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TenSinhVien)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GioiTinh)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.MaSinhVien">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.MaSinhVien">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.MaSinhVien">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
